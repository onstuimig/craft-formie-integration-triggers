{
	"name": "onstuimig/craft-formie-integration-triggers",
	"description": "Allow waiting for specific events before triggering Formie integrations",
	"version": "1.0.0",
	"type": "craft-plugin",
	"license": "proprietary",
	"support": {
		"issues": "https://github.com/onstuimig/craft-formie-integration-triggers/issues?state=open",
		"source": "https://github.com/onstuimig/craft-formie-integration-triggers",
		"docs": "https://github.com/onstuimig/craft-formie-integration-triggers",
		"rss": "https://github.com/onstuimig/craft-formie-integration-triggers/releases.atom"
	},
	"require": {
		"php": ">=8.0.2",
		"craftcms/cms": "^4.3.5||^5.0.0",
		"verbb/formie": "^2.0.0||^3.0.0"
	},
	"require-dev": {
		"craftcms/ecs": "dev-main",
		"craftcms/phpstan": "dev-main"
	},
	"autoload": {
		"psr-4": {
			"onstuimig\\formieintegrationtriggers\\": "src/"
		}
	},
	"extra": {
		"handle": "formie-integration-triggers",
		"name": "Formie Integration Triggers",
		"developer": "Onstuimig",
		"documentationUrl": "https://github.com/onstuimig/craft-formie-integration-triggers",
		"class": "onstuimig\\formieintegrationtriggers\\FormieIntegrationTriggers"
	},
	"scripts": {
		"check-cs": "ecs check --ansi",
		"fix-cs": "ecs check --ansi --fix",
		"phpstan": "phpstan --memory-limit=1G"
	},
	"config": {
		"sort-packages": true,
		"allow-plugins": {
			"yiisoft/yii2-composer": true,
			"craftcms/plugin-installer": true
		}
	}
}
